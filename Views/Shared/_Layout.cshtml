<!DOCTYPE html>
<html ng-app="policiesRoutingEx">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - GenLife</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("GenLife", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <!--Dont need this for now-->
                    @*<li>@Html.ActionLink("About", "About", "Home")</li>
                    <li>@Html.ActionLink("Contact", "Contact", "Home")</li>*@
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - GenLife</p>
        </footer>
    </div>

    @*@Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @RenderSection("scripts", required: false)*@

    <script src="~/Scripts/angularjs.1.8.0.js"></script>
    <script src="~/Scripts/ui-router.js"></script>
    @*<script src="app.js"></script>*@

    <!--scripts to work with policy table-->
    <script src="~/Assets/Policies/services/policies.services.js"></script>
    <script src="~/Assets/Policies/policies.module.js"></script>
    <script src="~/Assets/Policies/states/policies.states.js"></script>

    <!--For table to pdf-->
    <script src="~/Scripts/jquery-3.4.1.js"></script>
    <script src="~/Scripts/tableExportJs.js"></script>
    <script src="~/Scripts/jspdf.js"></script>
    <script src="~/Scripts/autotable.js"></script>
</body>
</html>
